<!DOCTYPE html>
<html>
<head>
  <title>Iitdatabases</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container">
    <div class="admin">
      <%= link_to 'Login', login_path %> |
      <%= link_to 'Logout', logout_path %>
      <% if flash[:notice] %>
        <p class="flash-notice"><%= flash[:notice] %></p>
      <% end %>
    </div>
    <%= yield %>
  </div>

</body>
</html>
